<script>		
 var items = new vis.DataSet({
    type: { start: 'ISODate', end: 'ISODate' }
  });

  // add items to the DataSet
  items.add([
    {id: 1, content: 'message 91<br>start', start: '2014-01-23'},
    {id: 2, content: 'message 92', start: '2014-01-18'},
    {id: 3, content: 'message 93', start: '2014-01-21'},
    {id: 4, content: 'message 94', start: '2014-01-19', end: '2014-01-24'},
    {id: 5, content: 'message 95', start: '2014-01-28', type:'point'},
    {id: 6, content: 'message 96', start: '2014-01-26'}
  ]);

  var container = document.getElementById('message_pipe');
  var options = {
    start: '2014-01-10',
    end: '2014-02-10',
    editable: true,
    showCurrentTime: true
  };

  var timeline = new vis.Timeline(container, items, options);
	timeline.fit();
 
